{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/bd",
      "destination": "/book-details.html"
    },
    {
      "source": "/bd/:path*",
      "destination": "/book-details.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/book-details.html",
      "destination": "/bd",
      "permanent": true
    }
  ]
} 